{
  "$schema": "https://titan.apimatic.io/api/build/schema",
  "buildFileVersion": "1",
  "generatePortal": {
    "tailIncludes": "<script defer src='./static/api-recipes/AutoRenewal.js'></script> <script defer src='./static/api-recipes/BlockUnblockCards.js'></script> <script defer src='./static/api-recipes/OrderNewCards.js'></script> <script defer src='./static/api-recipes/MoveCardsBetweenGroups.js'></script> <script defer src='./static/api-recipes/RequestPinReminders.js'></script> <script defer src='./static/api-recipes/ScheduleCardBlock.js'></script> <script defer src='./static/api-recipes/GetCardInformation.js'></script> <script>document.addEventListener('DOMContentLoaded', (event) => {  APIMaticDevPortal.ready(({ registerWorkflow }) => {  registerWorkflow( 'page:api-recipes/auto-renewal', 'Auto Renewal', AutoRenewal);  registerWorkflow( 'page:api-recipes/block-unblock-cards', 'Block/Unblock Cards', BlockUnblockCards);   registerWorkflow( 'page:api-recipes/order-new-cards', 'Order New Cards', OrderNewCards);  registerWorkflow( 'page:api-recipes/move-cards-between-groups', 'Move Cards Between Groups', MoveCardsBetweenGroups);  registerWorkflow( 'page:api-recipes/request-pin-reminders', 'Request Pin Reminders', RequestPinReminders);  registerWorkflow( 'page:api-recipes/schedule-card-block', 'Schedule Card Block', ScheduleCardBlock);  registerWorkflow( 'page:api-recipes/get-card-information', 'Get Card Information', GetCardInformation);  });  });</script>",
    "contentFolder": "./",
    "apiSpecPath": "./spec",
    "apiMergeSettings": {
      "type": "Full"
    },
    "languageConfig": {
      "http": {},
      "java": {
        "packageRepository": "Maven",
