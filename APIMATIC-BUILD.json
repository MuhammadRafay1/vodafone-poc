          "ArtifactId": "card-management-sdk",
          "PackageName": "Card Management SDK",
          "Version": "1.4.0"
        }
      },
      "csharp": {
        "packageRepository": "NuGet",
        "packageSettings": {
          "PackageName": "sdksio.CardManagementSDK",
          "Version": "1.4.0"
        }
      },
      "typescript": {
        "packageRepository": "Npm",
        "packageSettings": {
          "PackageName": "card-management-sdk",
          "Version": "1.4.0"
        }
      },
      "php": {
        "packageRepository": "Packagist",
        "packageSettings": {
          "VendorName": "shell",
          "PackageName": "card-management-sdk",
          "Version": "1.4.0"
        }
      },
      "python": {
